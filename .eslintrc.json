{
  "extends": ["prettier", "airbnb-base"],
  "plugins": ["prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-underscore-dangle": "off",
    "no-undef": "off",
    "no-restricted-syntax": "off",
    "quotes": "off",
    "no-lonely-if":"off",
    "no-use-before-define": "off"
  }
} 
