{
  "extends": ["prettier", "airbnb-base"],
  "plugins": ["prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-underscore-dangle": "error",
    "no-undef": "warn",
    "no-console":"warn",
    "no-restricted-syntax": "off",
    "quotes": "warn",
    "no-lonely-if": "off",
    "no-use-before-define": "warn",
    "object-shorthand": "warn",
    "func-names":"warn",
    "no-useless-catch":"warn",
    "consistent-return":"off"
  }
}
